          "showColumnConfigButton": true,
          "actions": {
            "delete": {
              "url": "/RegisterFormAdmin/UserRegisterForm/Delete"
            },
            "cActions": [
              {
                "type": "edit",
                "icon": "fa-pen",
                "prop": "id",
                "modalId": "UserRegisterFormEditModal",
                "url": "/RegisterFormAdmin/UserRegisterForm/GetById",
                "template": "function bindRow (curRow) { return '<span onclick=\"showEditModal('+ curRow.id +', \\'/RegisterFormAdmin/UserRegisterForm/GetById\\', \\'UserRegisterFormEditModal\\' , this)\" style=\"display:block;\" ><i style=\"color:white!important;\" class=\"fa fa-pen\" ></i></span>' };bindRow(data)"
              },
              {
                "template": "function bindRow (curRow) { return '<a target=\"_blank\" href=\"'+ curRow.url +'\" style=\"display:block;\" ><i style=\"color:white!important;\" class=\"fa fa-link\" ></i></span>' };bindRow(data)"
              }
            ]
          },
          "key": "id",
          "url": "/RegisterFormAdmin/UserRegisterForm/GetList",
          "itemPerPage": 10,
          "schema": {
            "data": "data",
            "total": "total"
          },
          "columns": [
            {
              "field": "row",
              "caption": "ردیف",
              "width": "50px",
              "textAlign": "center"
