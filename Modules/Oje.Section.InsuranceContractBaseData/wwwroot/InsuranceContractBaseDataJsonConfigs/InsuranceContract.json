                "url": "/InsuranceContractBaseData/InsuranceContract/GetById",
                "template": "function bindRow (curRow) { return '<span onclick=\"showEditModal('+ curRow.id +', \\'/InsuranceContractBaseData/InsuranceContract/GetById\\', \\'InsuranceContracteditGridModal\\' , this)\" style=\"display:block;\" ><i style=\"color:white!important;\" class=\"fa fa-pen\" ></i></span>' };bindRow(data)"
              }
            ]
          },
          "key": "id",
