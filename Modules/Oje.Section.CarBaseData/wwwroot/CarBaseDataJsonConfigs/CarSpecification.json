{
  "panels": [
    {
      "grids": [
        {
          "id": "userGrid",
          "topActions": [
            {
              "title": "افزودن",
              "onClick": "showModal('AddGridModal', this)", "type": "add", "modalId": "AddGridModal"
            }
          ],
          "exportToExcelUrl": "/CarBaseData/CarSpecification/Export",
          "showColumnConfigButton": true,
          "actions": {
            "delete": {
              "url": "/CarBaseData/CarSpecification/Delete"
            },
            "cActions": [
              {
                "type": "edit",
                "icon": "fa-pen",
                "prop": "id",
                "modalId": "editRoleModal",
                "url": "/CarBaseData/CarSpecification/GetById",
                "template": "function bindRow (curRow) { return '<span onclick=\"showEditModal('+ curRow.id +', \\'/CarBaseData/CarSpecification/GetById\\', \\'editRoleModal\\' , this)\" style=\"display:block;\" ><i style=\"color:white!important;\" class=\"fa fa-pen\" ></i></span>' };bindRow(data)"
              }
            ]
          },
          "key": "id",
          "url": "/CarBaseData/CarSpecification/GetList",
          "itemPerPage": 10,
          "schema": {
            "data": "data",
            "total": "total"
          },
          "columns": [
            {
              "field": "row",
              "caption": "ردیف",
              "width": "50px",
              "textAlign": "center"
            },
            {
              "field": "title",
              "caption": "عنوان",
              "search": {
                "searchType": "text"
              }
            },
            {
              "field": "carRomeRate",
              "caption": "نرخ اطاق بار",
              "search": {
                "searchType": "text"
              }
            },
            {
              "field": "specs",
              "caption": "خصوصیات خودرو",
              "search": {
                "searchType": "text"
              }
            },
            {
              "field": "isActive",
              "caption": "وضعیت",
              "search": {
                "searchType": "dropdown",
                "url": "/Core/BaseData/Get/IsActive",
                "textField": "title",
                "valueField": "id"
              }
            }
          ]
        }
      ],
      "moduals": [
        {
          "class": "modal-xl",
          "id": "AddGridModal",
          "title": "افزودن",
          "ctrls": [
            {
              "name": "id",
              "type": "hidden"
            },
            {
              "parentCL": "col-xl-4 col-lg-4 col-md-4 col-sm-6 col-xs-12",
              "name": "title",
              "type": "text",
              "label": "عنوان",
              "isRequired": true
            },
            {
              "parentCL": "col-xl-4 col-lg-4 col-md-4 col-sm-6 col-xs-12",
              "name": "carRoomRate",
              "type": "text",
              "label": "نرخ اطاق بار"
            },
            {
              "parentCL": "col-xl-4 col-lg-4 col-md-4 col-sm-6 col-xs-12",
              "name": "isActive",
              "type": "dropDown",
              "textfield": "title",
              "valuefield": "id",
              "dataurl": "/Core/BaseData/Get/IsActive",
              "label": "وضعیت"
            },
            {
              "parentCL": "col-xl-4 col-lg-4 col-md-4 col-sm-6 col-xs-12",
              "name": "catId",
              "id": "catId",
              "type": "dropDown",
              "textfield": "title",
              "valuefield": "id",
              "dataurl": "/CarBaseData/CarSpecification/GetCarSpecCategoryList",
              "label": "گروه بندی خصوصیات خودرو"
            },
            {
              "parentCL": "col-xl-4 col-lg-4 col-md-4 col-sm-6 col-xs-12",
              "name": "sysId",
              "id": "sysId",
              "type": "dropDown2",
              "textfield": "title",
              "valuefield": "id",
              "dataurl": "/CarBaseData/CarSpecification/GetBrandList",
              "label": "برند خودرو"
            },
            {
              "parentCL": "col-xl-12 col-lg-12 col-md-12 col-sm-12 col-xs-12",
              "name": "vehicleSpecIds",
              "type": "tokenBox2",
              "textfield": "title",
              "valuefield": "id",
              "dataurl": "/CarBaseData/CarSpecification/GetVehicleSpecList",
              "label": "خصوصیات خودرو",
              "isRequired": true,
              "exteraParameterIds": [ "catId", "sysId" ]
            }
          ],
          "actions": [
            {
              "title": "بستن",
              "onClick": "closeThisModal(this)",
              "class": "btn-secondary"
            },
            {
              "title": "دخیره",
              "onClick": "postModalData(this, 'userGrid', '/CarBaseData/CarSpecification/Create')",
              "class": "btn-primary"
            }
          ]
        },
        {
          "class": "modal-xl",
          "id": "editRoleModal",
          "title": "ویرایش",
          "ctrls": [
            {
              "name": "id",
              "type": "hidden"
            },
            {
              "parentCL": "col-xl-4 col-lg-4 col-md-4 col-sm-6 col-xs-12",
              "name": "title",
              "type": "text",
              "label": "عنوان",
              "isRequired": true
            },
            {
              "parentCL": "col-xl-4 col-lg-4 col-md-4 col-sm-6 col-xs-12",
              "name": "carRoomRate",
              "type": "text",
              "label": "نرخ اطاق بار"
            },
            {
              "parentCL": "col-xl-4 col-lg-4 col-md-4 col-sm-6 col-xs-12",
              "name": "isActive",
              "type": "dropDown",
              "textfield": "title",
              "valuefield": "id",
              "dataurl": "/Core/BaseData/Get/IsActive",
              "label": "وضعیت"
            },
            {
              "parentCL": "col-xl-4 col-lg-4 col-md-4 col-sm-6 col-xs-12",
              "name": "catId",
              "id": "catId2",
              "type": "dropDown",
              "textfield": "title",
              "valuefield": "id",
              "dataurl": "/CarBaseData/CarSpecification/GetCarSpecCategoryList",
              "label": "گروه بندی خصوصیات خودرو"
            },
            {
              "parentCL": "col-xl-4 col-lg-4 col-md-4 col-sm-6 col-xs-12",
              "name": "sysId",
              "id": "sysId2",
              "type": "dropDown2",
              "textfield": "title",
              "valuefield": "id",
              "dataurl": "/CarBaseData/CarSpecification/GetBrandList",
              "label": "برند خودرو"
            },
            {
              "parentCL": "col-xl-12 col-lg-12 col-md-12 col-sm-12 col-xs-12",
              "name": "vehicleSpecIds",
              "type": "tokenBox2",
              "textfield": "title",
              "valuefield": "id",
              "dataurl": "/CarBaseData/CarSpecification/GetVehicleSpecList",
              "label": "خصوصیات خودرو",
              "isRequired": true,
              "exteraParameterIds": [ "catId2", "sysId2" ]
            }
          ],
          "actions": [
            {
              "title": "بستن",
              "onClick": "closeThisModal(this)",
              "class": "btn-secondary"
            },
            {
              "title": "دخیره",
              "onClick": "postModalData(this, 'userGrid', '/CarBaseData/CarSpecification/Update')",
              "class": "btn-primary"
            }
          ]
        }
      ]
    }
  ]
}