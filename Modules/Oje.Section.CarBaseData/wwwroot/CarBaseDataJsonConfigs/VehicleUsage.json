{
  "panels": [
    {
      "grids": [
        {
          "id": "VehicleUsageGrid",
          "topActions": [
            {
              "title": "افزودن",
              "onClick": "showModal('VehicleUsageAddGridModal', this)",
              "type": "add",
              "modalId": "VehicleUsageAddGridModal"
            }
          ],
          "exportToExcelUrl": "/CarBaseData/VehicleUsage/Export",
          "showColumnConfigButton": true,
          "actions": {
            "delete": {
              "url": "/CarBaseData/VehicleUsage/Delete"
            },
            "cActions": [
              {
                "type": "edit",
                "icon": "fa-pen",
                "prop": "id",
                "modalId": "VehicleUsageModal",
                "url": "/CarBaseData/VehicleUsage/GetById",
                "template": "function bindRow (curRow) { return '<span onclick=\"showEditModal('+ curRow.id +', \\'/CarBaseData/VehicleUsage/GetById\\', \\'VehicleUsageModal\\' , this)\" style=\"display:block;\" ><i style=\"color:white!important;\" class=\"fa fa-pen\" ></i></span>' };bindRow(data)"
              }
            ]
          },
          "key": "id",
          "url": "/CarBaseData/VehicleUsage/GetList",
          "itemPerPage": 10,
          "schema": {
            "data": "data",
            "total": "total"
          },
          "columns": [
            {
              "field": "row",
              "caption": "ردیف",
              "width": "50px",
              "textAlign": "center"
            },
            {
              "field": "title",
              "caption": "عنوان",
              "search": {
                "searchType": "text"
              }
            },
            {
              "field": "carTypes",
              "caption": "کاربری خودرو",
              "search": {
                "searchType": "dropdown",
                "url": "/CarBaseData/VehicleUsage/GetCarTypeList",
                "textField": "title",
                "valueField": "id"
              }
            },
            {
              "field": "bodyPercent",
              "caption": "درصد بدنه",
              "search": {
                "searchType": "text"
              }
            },
            {
              "field": "thirdPercent",
              "caption": "درصد  ثالث",
              "search": {
                "searchType": "text"
              }
            },
            {
              "field": "isActive",
              "caption": "وضعیت",
              "search": {
                "searchType": "dropdown",
                "url": "/Core/BaseData/Get/IsActive",
                "textField": "title",
                "valueField": "id"
              }
            }
          ]
        }
      ],
      "moduals": [
        {
          "class": "",
          "id": "VehicleUsageAddGridModal",
          "title": "افزودن",
          "ctrls": [
            {
              "name": "id",
              "type": "hidden"
            },
            {
              "parentCL": "col-xl-12 col-lg-12 col-md-12 col-sm-12 col-xs-12",
              "name": "title",
              "type": "text",
              "label": "عنوان",
              "isRequired": true
            },
            {
              "parentCL": "col-xl-12 col-lg-12 col-md-12 col-sm-12 col-xs-12",
              "name": "carTypeIds",
              "type": "tokenBox",
              "textfield": "title",
              "valuefield": "id",
              "dataurl": "/CarBaseData/VehicleUsage/GetCarTypeList",
              "label": "کاربری خودرو",
              "isRequired": true
            },
            {
              "parentCL": "col-xl-12 col-lg-12 col-md-12 col-sm-12 col-xs-12",
              "name": "bodyPercent",
              "type": "text",
              "label": "درصد مورد استفاده بدنه"
            },
            {
              "parentCL": "col-xl-12 col-lg-12 col-md-12 col-sm-12 col-xs-12",
              "name": "thirdPartyPercent",
              "type": "text",
              "label": "درصد مورد استفاده ثالث"
            },
            {
              "parentCL": "col-xl-12 col-lg-12 col-md-12 col-sm-12 col-xs-12",
              "name": "isActive",
              "type": "dropDown",
              "textfield": "title",
              "valuefield": "id",
              "dataurl": "/Core/BaseData/Get/IsActive",
              "label": "وضعیت"
            }
          ],
          "actions": [
            {
              "title": "بستن",
              "onClick": "closeThisModal(this)",
              "class": "btn-secondary"
            },
            {
              "title": "ذخیره",
              "onClick": "postModalData(this, 'VehicleUsageGrid', '/CarBaseData/VehicleUsage/Create')",
              "class": "btn-primary"
            }
          ]
        },
        {
          "class": "",
          "id": "VehicleUsageModal",
          "title": "ویرایش",
          "ctrls": [
            {
              "name": "id",
              "type": "hidden"
            },
            {
              "parentCL": "col-xl-12 col-lg-12 col-md-12 col-sm-12 col-xs-12",
              "name": "title",
              "type": "text",
              "label": "عنوان",
              "isRequired": true
            },
            {
              "parentCL": "col-xl-12 col-lg-12 col-md-12 col-sm-12 col-xs-12",
              "name": "carTypeIds",
              "type": "tokenBox",
              "textfield": "title",
              "valuefield": "id",
              "dataurl": "/CarBaseData/VehicleUsage/GetCarTypeList",
              "label": "کاربری خودرو",
              "isRequired": true
            },
            {
              "parentCL": "col-xl-12 col-lg-12 col-md-12 col-sm-12 col-xs-12",
              "name": "bodyPercent",
              "type": "text",
              "label": "درصد مورد استفاده بدنه"
            },
            {
              "parentCL": "col-xl-12 col-lg-12 col-md-12 col-sm-12 col-xs-12",
              "name": "thirdPartyPercent",
              "type": "text",
              "label": "درصد مورد استفاده ثالث"
            },
            {
              "parentCL": "col-xl-12 col-lg-12 col-md-12 col-sm-12 col-xs-12",
              "name": "isActive",
              "type": "dropDown",
              "textfield": "title",
              "valuefield": "id",
              "dataurl": "/Core/BaseData/Get/IsActive",
              "label": "وضعیت"
            }
          ],
          "actions": [
            {
              "title": "بستن",
              "onClick": "closeThisModal(this)",
              "class": "btn-secondary"
            },
            {
              "title": "ذخیره",
              "onClick": "postModalData(this, 'VehicleUsageGrid', '/CarBaseData/VehicleUsage/Update')",
              "class": "btn-primary"
            }
          ]
        }
      ]
    }
  ]
}